{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "constraints": {
    "go": "1.24"
  },
  "extends": [
    "config:recommended",
    ":semanticCommitTypeAll(build)",
    ":separatePatchReleases"
  ],
  "ignorePresets": [
    ":combinePatchMinorReleases",
    ":prHourlyLimit2",
    ":semanticPrefixFixDepsChoreOthers"
  ],
  "enabledManagers": [
    "gomod"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "go"
      ],
      "addLabels": [
        "go"
      ]
    }
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ]
}
